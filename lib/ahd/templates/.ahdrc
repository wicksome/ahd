[conf]

; App for editing header.
; vi is not supported, and default app is editor(similar to vi).
; default editor is https://www.npmjs.com/package/editor.
; editor = /usr/bin/atom

[location]

; Location the apachectl for restart apache.
; apachectl = /usr/sbin/apachectl

; Location httpd.conf for save custom header
; httpdconf = /etc/apache2/httpd.conf